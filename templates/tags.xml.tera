
{% extends "layout.xml.tera" %}
{% block content %}

<title>Tags | {{ tags | length() }}</title>

  {% for tag in tags %}
  <entry>
    <title>{{ tag.name }}</title>
    <id>ORCA:Tags{{ tag.id }}</id>
    <link href="/tags/{{ tag.id }}" type="application/atom+xml;profile=opds-catalog;kind=navigation"/>
    <content type="text">Books tagged {{ tag.name }}</content>
  </entry>
  {% endfor %}

{% endblock content %}
